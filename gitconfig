[user]
	name = dobashi
	email = dobashi@lavans.jp
[alias]
	a = add .
	b = branch
	br = branch -r
	c = commit
	co = checkout
    df = diff
    dfc = diff --cached
    dfh = diff HEAD^ HEAD 
	l  = log
	lo = log --oneline
	lod = log --pretty='format:%C(yellow)%h%C(red)%d %C(green)%as %C(blue)%<(10,trunc)%an %C(reset)%s'
    lg = log --graph --all --format='%x09%C(red)%d %Cblue%<(12,trunc)%an %C(green)%aI %C(yellow)%h %Creset%s'
	p  = pull
	pr  = pull --rebase
	pd = pull --recurse-submodules origin develop
	pm = pull --recurse-submodules origin master
	ps  = push
	psd = push origin develop
	psm = push origin master
	r = remote -v
    rc = rebase --continue
	s = status
	su = submodule update
	sw = switch
[pull]
	default = current
	rebase = true
#	rebase = merges
[push]
	default = current
[init]
	defaultBranch = develop
